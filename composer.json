{
    "name": "levaral-dev/core",
    "description": "Levaral foundation package for Laravel Framework",
    "minimum-stability": "dev",
    "license": "MIT",
    "require": {
        "php": "^7.0",
        "gossi/php-code-generator": "^0.5.0",
        "geoip2/geoip2": "~2.0",
        "doctrine/dbal": "^2.5",
        "laravel/framework": "5.7.*",
        "guzzlehttp/guzzle": "~6.0",
        "doctrine/common": "^2.9@dev"
    },
    "autoload": {
        "psr-4": {
            "Levaral\\Core\\": "src/"
        },
        "files": ["src/helpers.php"]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Levaral\\Core\\CoreServiceProvider"
            ]
        }
    }
}
